{
  "name": "claudecode",
  "version": "2.1.7",
  "private": true,
  "description": "Claude Code - Reconstructed TypeScript Source",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "symbol:lookup": "tsx scripts/symbol-lookup.ts",
    "symbol:find": "tsx scripts/symbol-find.ts",
    "symbol:chunk": "tsx scripts/symbol-chunk.ts"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "esbuild": "^0.20.0",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "turbo": "^2.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "workspaces": [
    "packages/*"
  ]
}
