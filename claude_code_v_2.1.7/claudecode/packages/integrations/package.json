{
  "name": "@claudecode/integrations",
  "version": "2.1.7",
  "description": "External integrations for Claude Code",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./mcp": {
      "import": "./dist/mcp/index.js",
      "types": "./dist/mcp/index.d.ts"
    },
    "./ide": {
      "import": "./dist/ide/index.js",
      "types": "./dist/ide/index.d.ts"
    },
    "./chrome": {
      "import": "./dist/chrome/index.js",
      "types": "./dist/chrome/index.d.ts"
    },
    "./lsp-server": {
      "import": "./dist/lsp-server/index.js",
      "types": "./dist/lsp-server/index.d.ts"
    },
    "./background-agents": {
      "import": "./dist/background-agents/index.js",
      "types": "./dist/background-agents/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/mcp/index.ts src/ide/index.ts src/chrome/index.ts src/lsp-server/index.ts src/background-agents/index.ts --format esm --dts",
    "dev": "tsup src/index.ts src/mcp/index.ts src/ide/index.ts src/chrome/index.ts src/lsp-server/index.ts src/background-agents/index.ts --format esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@claudecode/shared": "workspace:*",
    "@claudecode/core": "workspace:*",
    "@claudecode/platform": "workspace:*",
    "zod": "^3.22.4",
    "ink": "^4.4.1",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
